{
  "name": "react-native-rtmp-livestream",
  "version": "1.0.0",
  "description": "RTMP live streaming module for React Native & Expo (Expo Module API)",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "build",
    "android",
    "ios",
    "plugin.js",
    "*.podspec"
  ],
  "expo": {
    "autolinking": {
      "ios": {
        "modules": [
          "RTMPLiveStreamModule"
        ]
      },
      "android": {
        "modules": [
          "video.api.expo.rtmp.RTMPLiveStreamModule"
        ]
      }
    },
    "plugins": [
      "./plugin"
    ]
  },
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "expo-module-scripts": "^5.0.7"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "peerDependenciesMeta": {
    "react-native-web": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arvindra1/react-native-rtmp-livestream.git"
  },
  "keywords": [
    "react-native",
    "expo",
    "rtmp",
    "livestream",
    "camera",
    "video-stream"
  ],
  "author": "Arvindra",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "extends": [
      "expo-module-scripts/eslint"
    ],
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
